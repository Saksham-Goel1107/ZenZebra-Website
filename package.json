{
  "name": "zenzebra-site",
  "version": "1.0.0",
  "private": true,
  "author": "Saksham-Goel1107",
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@heroicons/react": "latest",
    "@sentry/nextjs": "^10.38.0",
    "firebase": "^12.8.0",
    "gsap": "^3.14.2",
    "lucide-react": "latest",
    "motion": "latest",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "swiper": "latest"
  },
  "devDependencies": {
    "@eslint/eslintrc": "latest",
    "@tailwindcss/postcss": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "eslint": "8.57.0",
    "eslint-config-next": "latest",
    "framer-motion": "latest",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "require-in-the-middle": "^8.0.1",
    "tailwindcss": "latest",
    "typescript": "latest"
  }
}
